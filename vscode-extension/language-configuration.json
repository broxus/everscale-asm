{
  "comments": {
    "lineComment": "//"
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"]
  ],
  "autoClosingPairs": [
    { "open": "{", "close": "}" },
    { "open": "[", "close": "]" },
    { "open": "(", "close": ")" }
  ],
  "autoCloseBefore": ";:.,=}])>` \n\t",
  "indentationRules": {
    "increaseIndentPattern": "[{\\[\\(]\\s*$",
    "decreaseIndentPattern": "^[^ ]$"
  }
}
